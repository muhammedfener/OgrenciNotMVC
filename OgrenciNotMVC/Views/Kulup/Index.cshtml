@using OgrenciNotMVC.Models.EntityFramework
@model List<TBLKULUPLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table table-bordered">
    <tr>
        <th>KULUP ID</th>
        <th>KULUP ADI</th>
        <th>İŞLEMLER</th>
    </tr>
    <tbody>
        @foreach (var kulup in Model)
        {
            <tr>
                <td>
                    @kulup.KULUPID
                </td>
                <td>
                    @kulup.KULUPAD
                </td>
                <td>
                    <a href="/Kulup/Sil/@kulup.KULUPID" class="btn btn-danger">Sil</a>
                    <a href="/Kulup/KulupGetir/@kulup.KULUPID" class="btn btn-success">Güncelle</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<a href="/Kulup/YeniKulup" class="btn btn-primary">Yeni Kulüp Ekle</a>